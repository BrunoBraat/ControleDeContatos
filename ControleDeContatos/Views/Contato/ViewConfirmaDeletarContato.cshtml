@{
    ViewData["Title"] = "Confirmação de deletar Contato";
}

<div class="text-center">
    <h4 class="display-4">Realmente deseja apagar o contato Reis ?</h4>
    <br/>
    <div class="btn-group" role="group">
        <a class="btn btn-secondary" role="button" asp-controller="Contato" asp-action="ViewIndexContato">Cancelar</a>
        <a class="btn btn-danger" role="button" asp-controller="Contato" asp-action="ViewDeletarContato">Confirmar</a>
    </div>
</div><!--text-center-->
 