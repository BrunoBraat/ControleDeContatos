@{
    ViewData["Title"] = "Editar de Contato";
}

<div class="text-center">

    <h1 class="display-4">Editar contato</h1>
    <br />
</div><!--text-center-->


<form>
    <div class="form-group">
        <label for="Nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato...">
    </div>

    <div class="form-group">
        <label for="email">E-mail</label>
        <input type="email" class="form-control" placeholder="Digite o e-mail do contato...">
    </div>

    <div class="form-group">
        <label for="celular">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o número de celular do contato...">
    </div>

    <a class="btn btn-secondary" asp-controller="contato" asp-action="ViewIndexContato" role="button">Voltar</a>
    <button type="submit" class="btn btn-primary">Salvar</button>
</form>


